    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="caracter" type="String" array="False" size=""/>
            <output expression="&quot;Introduce un caracter&quot;" newline="True"/>
            <input variable="caracter"/>
            <if expression="caracter != &quot;A&quot; and caracter !=&quot;E&quot; and caracter != &quot;I&quot; and caracter !=&quot;O&quot; and caracter !=&quot;U&quot;">
                <then>
                    <output expression="&quot;No es una vocal &quot; &amp; caracter" newline="True"/>
                </then>
                <else>
                    <if expression="caracter == &quot;A&quot;">
                        <then>
                            <output expression="&quot;Es la vocal &quot; &amp; caracter" newline="True"/>
                        </then>
                        <else>
                            <if expression="caracter ==&quot;E&quot;">
                                <then>
                                    <output expression="&quot;Es la vocal &quot; &amp; caracter" newline="True"/>
                                </then>
                                <else>
                                    <if expression="caracter == &quot;I&quot;">
                                        <then>
                                            <output expression="&quot;Es la vocal &quot; &amp; caracter" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="caracter ==&quot;O&quot;">
                                                <then>
                                                    <output expression="&quot;Es la vocal &quot; &amp; caracter" newline="True"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;Es la vocal &quot; &amp; caracter" newline="True"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
